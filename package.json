{
  "name": "fpga-pin-planner",
  "version": "1.0.10",
  "description": "Advanced FPGA Pin Assignment and Management Tool with Voltage/IO Standard Selection",
  "main": "dist-electron/main.js",
  "homepage": "./",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && electron .\"",
    "electron:pack": "npm run build && electron-builder",
    "electron:dist": "npm run build && electron-builder --publish=never",
    "release:patch": "node scripts/prepare-release.mjs patch",
    "release:minor": "node scripts/prepare-release.mjs minor",
    "release:major": "node scripts/prepare-release.mjs major",
    "build:full": "pwsh -ExecutionPolicy Bypass -File scripts/full-build-install.ps1",
    "build:quick": "pwsh -ExecutionPolicy Bypass -File scripts/quick-build.ps1",
    "build:extension-only": "pwsh -ExecutionPolicy Bypass -File scripts/quick-build.ps1 -OnlyExtension",
    "install:extension": "pwsh -ExecutionPolicy Bypass -File scripts/full-build-install.ps1 -SkipMainBuild -SkipExtensionBuild"
  },
  "keywords": [
    "fpga",
    "pin-assignment",
    "pin-planner",
    "xilinx",
    "intel-quartus",
    "voltage-io-standards",
    "differential-pairs",
    "batch-operations",
    "constraint-validation",
    "react",
    "typescript",
    "vite"
  ],
  "author": "FPGA Pin Planner Team",
  "license": "MIT",
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/xlsx": "^0.0.35",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "canvas": "^3.1.2",
    "concurrently": "^8.2.0",
    "electron": "^25.0.0",
    "electron-builder": "^24.0.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "jsdom": "^26.1.0",
    "typescript": "^5.0.0",
    "vite": "^4.4.0",
    "vite-plugin-electron": "^0.12.0",
    "vitest": "^3.2.4",
    "wait-on": "^7.0.1"
  },
  "dependencies": {
    "@types/papaparse": "^5.3.0",
    "immer": "^10.0.0",
    "konva": "^9.2.0",
    "lucide-react": "^0.263.0",
    "papaparse": "^5.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-konva": "^18.2.0",
    "xlsx": "^0.18.5",
    "zustand": "^4.4.0"
  },
  "build": {
    "appId": "com.fpga-pin-planner.app",
    "productName": "FPGA Pin Planner",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*",
      "node_modules/**/*"
    ],
    "mac": {
      "icon": "build/icon.icns"
    },
    "win": {
      "icon": "build/icon.ico"
    },
    "linux": {
      "icon": "build/icon.png"
    }
  }
}
